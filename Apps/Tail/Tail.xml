<?xml version="1.0" ?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface"
           uri="http://www.kerofin.demon.co.uk/2005/interfaces/Tail"
	   main="Tail/AppRun">
  <name>Tail</name>
  <summary>Monitor a text file</summary>
  <description>
This program displays the contents of a text file, updating as the text file is updated.
  </description>
  <icon type='image/png' href='http://www.kerofin.demon.co.uk/icons/Tail.png'/>
<group>
    <requires interface="http://www.kerofin.demon.co.uk/2005/interfaces/ROX-CLib">
      <environment insert="." name="LIBDIRPATH"/>
    </requires>
    <implementation id=".." released="snapshot"
		stability="buggy" version="2.1.3" platform="Linux-ix86"/>
  </group>
<feed-for interface='http://www.kerofin.demon.co.uk/2005/interfaces/Tail'/>
</interface>
