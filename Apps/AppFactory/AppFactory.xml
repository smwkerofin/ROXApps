<?xml version="1.0" ?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface"
           uri="http://www.kerofin.demon.co.uk/2005/interfaces/AppFactory"
	   main="AppFactory/AppRun">
  <name>AppFactory</name>
  <summary>Construct AppDir wrappers for applications.</summary>
  <description>
This program automates the creation of ROX wrappers for non-ROX
programs

  </description>
  <icon type='image/png' href='http://www.kerofin.demon.co.uk/icons/AppFactory.png'/>
<group>
    <requires interface="http://www.kerofin.demon.co.uk/2005/interfaces/ROX-CLib">
      <environment insert="." name="LIBDIRPATH"/>
    </requires>
    <implementation id=".."
		stability="testing" version="2.1.5" platform="Linux-ix86"/>
  </group>
  <feed-for interface='http://www.kerofin.demon.co.uk/2005/interfaces/AppFactory'/>
</interface>
