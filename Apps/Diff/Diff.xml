<?xml version="1.0" ?>
<interface xmlns="http://zero-install.sourceforge.net/2004/injector/interface"
           uri="http://www.kerofin.demon.co.uk/2005/interfaces/Diff"
	   main="Diff/AppRun">
  <name>Diff</name>
  <summary>display the difference between two text files.</summary>
  <description>
This program displays a coloured context diff between two files.  You
drop the files from the filer into the upper 2 windows.

  </description>
  <icon type='image/png' href='http://www.kerofin.demon.co.uk/icons/Diff.png'/>
<group>
    <requires interface="http://www.kerofin.demon.co.uk/2005/interfaces/ROX-CLib">
      <environment insert="." name="LIBDIRPATH"/>
    </requires>
    <implementation id=".." released='Snapshot' 
		stability="testing" version="2.1.4" platform="Linux-ix86"/>
  </group>
<feed-for interface='http://www.kerofin.demon.co.uk/2005/interfaces/Diff'/>
</interface>
